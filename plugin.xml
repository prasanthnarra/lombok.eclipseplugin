<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
<extension
         point="org.eclipse.ltk.core.refactoring.refactoringContributions">
      <contribution
            class="lombok.eclipse.refactoring.LombokRefactoringContribution"
            id="org.projectlombok.refactor">
      </contribution>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="lombok.eclipse.methodRefactor"
            objectClass="org.eclipse.jdt.core.IJavaElement">
         <action
               class="lombok.eclipse.actions.LombokRefactorAction"
               id="lombok.refactoring.test"
               label="Lombok Test Refactor"
               menubarPath="org.eclipse.jdt.ui.refactoring.menu/codingGroup"
               style="push">
         </action>
      </objectContribution>
   </extension> 
</plugin>
